def calculate_y(x,a,b,c):
    if x<1:
        return 2*a*x+3*b-c
    elif x>1:
        return 4*x+b*x+a
    else:
        return 3*x**2+x-1
x_values=range(-1,6)
a=float(input("enter a:"))
b=float(input("enter b:"))
c=float(input("enter c:"))
for x in x_values:
    y=calculate_y(x,a,b,c)
    print(x)
    print(y)
